{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "geoserver": {
    "url": "",
    "auth": {
      "username": "",
      "password": ""
    },
    "dataStore": {
      "username": "",
      "password": "",
      "host": "",
      "port": "",
      "schema": "",
      "dbName": "",
      "dbType": "",
      "sslMode": ""
    },
    "srs": "",
    "numOfDecimals": 100
  },
  "httpRetry": {
    "attempts": 5,
    "delay": "exponential",
    "shouldResetTimeout": true
  },
  "disableHttpClientLogs": false
}
