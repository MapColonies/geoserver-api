{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "geoserver": {
    "url": "http://localhost:8080/geoserver",
    "auth": {
      "username": "admin",
      "password": "geoserver"
    },
    "dataStore": {
      "username": "postgres",
      "password": "postgres",
      "host": "localhost",
      "port": "5432",
      "schema": "polygon_parts",
      "dbName": "postgres",
      "dbType": "postgis",
      "sslMode": "DISABLE"
    },
    "srs": "EPSG:4326",
    "numOfDecimals": 100
  },
  "httpRetry": {
    "attempts": 5,
    "delay": "exponential",
    "shouldResetTimeout": true
  },
  "disableHttpClientLogs": false
}
